[cylc]
    UTC mode = True
[scheduling]
    initial cycle point = 2000-01-01T00Z
    [[dependencies]]
[scheduling]
    [[dependencies]]
        [[[T00]]]
            graph = yesterday => today
        [[[T06]]]
            graph = cockrel_crows
[runtime]
    [[root]]
        script = echo default
[runtime]
    [[yesterday]]
        script = echo yesterday
    [[today]]
        script = echo today
    [[cockrel_crows]]
        script = echo "cockadooldledoo"
